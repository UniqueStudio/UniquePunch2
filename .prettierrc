{
  "printWidth": 120,
  "tabWidth": 4,
  "overrides": [
    {
      "files": ["*.json", ".eslintrc", ".tslintrc", ".prettierrc", ".tern-project"],
      "options": {
        "parser": "json",
        "tabWidth": 2
      }
    },
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": ["*.tsx", ".html", ".vue"],
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
